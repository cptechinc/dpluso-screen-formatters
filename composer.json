{
	"name": "cptechinc/dpluso-screen-formatters",
    "type": "library",
    "description": "Screen Formatter Classes for Dplus Online",
    "keywords": ["ii", "ci", "vi", "screen-formatter", "table-screen"],
    "homepage": "http://github.com/cptechinc/dpluso-screen-formatters",
	"require": {
        "cptechinc/dplus-base": "dev-master",
        "cptechinc/dplus-processwire": "dev-master",
        "cptechinc/dplus-order": "dev-master",
        "cptechinc/dplus-content": "dev-master"
    },
	"autoload": {
		"psr-4":{
			"Dplus\\Dpluso\\ScreenFormatters\\CI\\": "src/ci/",
			"Dplus\\Dpluso\\ScreenFormatters\\II\\": "src/ii/",
			"Dplus\\Dpluso\\ScreenFormatters\\VI\\": "src/vi/",
			"Dplus\\Dpluso\\ScreenFormatters\\Item\\": "src/item/"
		},
		"classmap": [
			"src/ScreenFormatterFactory.class.php",
			"src/TableScreen.class.php",
			"src/TableScreenFormatter.class.php"
		]
	}
}
